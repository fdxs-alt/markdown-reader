{
  "name": "markdown-reader",
  "version": "1.0.0",
  "main": "./dist/main.js",
  "repository": "https://github.com/fdxs-alt/markdown-reader.git",
  "author": "Jakub Sukiennik <kuba1207710@gmail.com>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint . --ext .ts",
    "start": "tsc -p . && electron-forge start",
    "package": "tsc -p .  && electron-forge package",
    "make": "tsc -p . && electron-forge make"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "@types/marked": "^1.1.0",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "electron": "^10.1.5",
    "eslint": "^7.12.1",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0",
    "marked": "^1.2.2"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "markdown_reader"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
